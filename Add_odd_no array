#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;

int main() {
    int n;
    cin >> n;

    int arr[100]; // Fixed size array
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    int sum = 0; 

    for (int i = 0; i < n; i++) {
        if (arr[i] % 2 != 0) {
            sum += arr[i]; // Add to sum if odd
        }
    }

    cout << sum << endl; 
    return 0;
}


